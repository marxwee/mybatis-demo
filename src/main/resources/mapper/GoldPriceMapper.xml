<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.edu.hzau.mybatis.demo.mapper.GoldPriceMapper">
    <select id="selectById" resultType="cn.edu.hzau.mybatis.demo.model.GoldPrice">
        select id, price, timestamp
        from gold_price
        where id = #{id}
    </select>
</mapper>
